{
  "name": "@forgewright/ai",
  "version": "0.1.0",
  "description": "AI analysis engine for Forgewright - work units, changelogs, readiness scoring",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target bun",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "release",
    "changelog",
    "ai",
    "llm",
    "claude",
    "openai",
    "gemini",
    "work-units",
    "automation"
  ],
  "author": "Arakiss",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Arakiss/forgewright.git",
    "directory": "packages/ai"
  },
  "bugs": {
    "url": "https://github.com/Arakiss/forgewright/issues"
  },
  "homepage": "https://github.com/Arakiss/forgewright#readme",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@forgewright/core": "workspace:*",
    "ai": "catalog:",
    "@ai-sdk/anthropic": "catalog:",
    "@ai-sdk/openai": "catalog:",
    "@ai-sdk/google": "catalog:",
    "@ai-sdk/xai": "catalog:",
    "@ai-sdk/mistral": "catalog:",
    "@ai-sdk/openai-compatible": "catalog:",
    "ollama-ai-provider-v2": "^1.1.3",
    "zod": "catalog:"
  },
  "devDependencies": {
    "typescript": "catalog:",
    "@types/bun": "latest"
  },
  "publishConfig": {
    "access": "public"
  }
}
